{
	"root": true,
	"extends": [
        "airbnb-base",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"rules": {
		"no-const-assign": 0,
		"no-unused-vars": 0,
		"class-methods-use-this": 0,
		"consistent-return": 1,
		"func-names": ["error", "never"],
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"useTabs": true,
				"semi": true,
				"trailingComma": "all",
				"bracketSpacing": true,
				"printWidth": 100,
				"endOfLine": "auto"
			}
		]
	}
}